    <!-- Classes -->
    
    <owl:Class rdf:about="#Root">
        <rdfs:label>Root</rdfs:label>
        <rdfs:comment>Root object representing the main data structure</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="#Person">
        <rdfs:label>Person</rdfs:label>
        <rdfs:comment>Osoba definovaná registračnými údajmi</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="#Address">
        <rdfs:label>Address</rdfs:label>
        <rdfs:comment>Adresa zhromažďuje kompletnú adresu, ktorú užívateľ poskytol, vrátane ulice, mesta/obce, PSČ a prípadne aj krajiny.</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="#BiometricData">
        <rdfs:label>BiometricData</rdfs:label>
        <rdfs:comment>Biometric data of a person</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="#ListOfGroceryShoppings">
        <rdfs:label>ListOfGroceryShoppings</rdfs:label>
        <rdfs:comment>ListOfGroceryShopping predstavuje kompletný zoznam (históriu) všetkých nákupov potravín daného užívateľa.</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="#GroceryShopping">
        <rdfs:label>GroceryShopping</rdfs:label>
        <rdfs:comment>A grocery shopping instance</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="#GroceryItem">
        <rdfs:label>GroceryItem</rdfs:label>
        <rdfs:comment>An item in a grocery shopping</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="#RawQA">
        <rdfs:label>RawQA</rdfs:label>
        <rdfs:comment>Obsahuje údaje o sentimentálnej analýze prebiehajúcej konverzácie</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="#PositionTracking">
        <rdfs:label>PositionTracking</rdfs:label>
