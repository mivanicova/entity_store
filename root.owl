                    <owl:NamedIndividual rdf:about="#modrá"/>
                    <owl:NamedIndividual rdf:about="#zelená"/>
                    <owl:NamedIndividual rdf:about="#hnedá"/>
                    <owl:NamedIndividual rdf:about="#sivá"/>
                    <owl:NamedIndividual rdf:about="#mix"/>
                </owl:oneOf>
            </owl:Class>
        </owl:equivalentClass>
    </owl:Class>
    
    <!-- QA Sentiment Enumeration -->
    <owl:Class rdf:about="#QASentiment">
        <rdfs:label>QASentiment</rdfs:label>
        <owl:equivalentClass>
            <owl:Class>
