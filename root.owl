        <rdfs:label>Nehnutelnost</rdfs:label>
        <rdfs:comment>Nehnuteľnosť definovaná schémou</rdfs:comment>
    </owl:Class>
    
    <!-- Eye Color Enumeration -->
    <owl:Class rdf:about="#EyeColor">
