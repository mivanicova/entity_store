    </owl:Class>
    
    <owl:Class rdf:about="#GPSPoint">
        <rdfs:label>GPSPoint</rdfs:label>
        <rdfs:comment>A GPS point with coordinates and timestamp</rdfs:comment>
    </owl:Class>
    
